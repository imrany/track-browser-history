
{
  "manifest_version": 3,
  "name": "My History Tracker",
  "description": "Tracks browsing history",
  "version": "0.1.0",
  "author":"imrany",
  "permissions": ["history","favicon","webRequestBlocking"],
  "chrome_url_overrides":{
    "history":"index.html"
  }, 
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon.ico",
      "48": "icon.ico",
      "128": "icon.ico"
    }
  }
}
